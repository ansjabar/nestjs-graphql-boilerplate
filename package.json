{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "env-cmd -f .env ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm-test": "env-cmd -f .env.test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "make:migration": "read -p 'Migration Name: ' fileName && yarn run typeorm migration:create src/database/migrations/\"$fileName\"",
    "make:seed": "read -p 'Seeder Name: ' fileName && yarn run typeorm migration:create src/database/seeders/\"$fileName\"",
    "db:migrate": "npm run typeorm migration:run  -- -d ./src/database/migration.data-source.ts",
    "db:seed": "npm run typeorm migration:run -- -d ./src/database/seeder.data-source.ts",
    "key:generate": "ts-node ./src/utils/env-writers/app-key.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@bugsnag/js": "^7.23.0",
    "@bugsnag/plugin-express": "^7.22.7",
    "@bull-board/api": "^5.11.0",
    "@bull-board/express": "^5.11.0",
    "@bull-board/nestjs": "^5.11.0",
    "@codebrew/nestjs-storage": "^0.1.7",
    "@nestjs-modules/mailer": "1.10.3",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.1",
    "@slynova/flydrive": "^1.0.3",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.3.0",
    "bull": "^4.12.0",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.7",
    "env-cmd": "^10.1.0",
    "file-type": "16.5.4",
    "googleapis": "^144.0.0",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "13",
    "graphql-ws": "^5.15.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nestjs-i18n": "^10.4.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.6",
    "stream-to-buffer": "^0.1.0",
    "typeorm": "^0.3.19",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "@types/express": "^4.17.17",
    "@types/graphql-upload": "8",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "json-to-graphql-query": "^2.2.5",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 60000,
    "rootDir": "./",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "dist",
      "node_modules",
      "coverage",
      "src/database",
      "src/config",
      "src/utils",
      "src/main.ts",
      "src/common/columns",
      ".*\\.processor\\.ts",
      ".*\\.plugin\\.ts",
      ".*\\.repository\\.ts",
      ".*\\.module\\.ts",
      ".*\\.entity\\.ts",
      ".*\\.decorator\\.ts",
      ".*\\.validator\\.ts",
      ".*\\.dto\\.ts",
      ".*\\.type\\.ts",
      ".*\\.subscription\\.ts",
      ".*\\.constant\\.ts",
      "queue.service.ts",
      "index.ts"
    ],
    "testEnvironment": "node",
    "globalSetup": "<rootDir>/test/setup/jest-global.setup.ts",
    "globalTeardown": "<rootDir>/test/setup/jest-global.teardown.ts"
  }
}
